{
    "manifest_version": 2,
    "name": "Astroid",
    "version": "1.0",
    "description": "The Easiest Way To Inject Astroid V3!",
    "permissions": [
      "activeTab"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": true
      },
      "icons": {
        "16": "logo.png",
        "32": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    }
  }
  